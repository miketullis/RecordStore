@model RecordStore.UI.MVC.Models.ContactViewModel

@* Above is a @model declaration. This makes the View strongly-typed.
    This means that this View can only use one model for data. *@

@{
    ViewBag.Title = "Email Confirmation";
}

<h2>@ViewBag.Title</h2>

<h4>Thank you! We have received the following message:</h4>
<p>@Model.Message</p>
<h4>We will respond to @Model.Email as soon as possible.</h4>
